{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "type": "object",
  "required": [
    "productId"
  ],
  "properties": {
    "attributes": {
      "title": "Attribut",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "has_archives": { "title": "Har arkiv", "type": "boolean" },
          "id": { "title": "Attribut-ID", "type": "integer" },
          "name": { "title": "Namn", "type": "string" },
          "option": { "title": "Valt alternativ", "type": "string" },
          "order_by": { "title": "Sortering", "type": "string" },
          "slug": { "title": "Slug", "type": "string" },
          "type": { "title": "Typ", "type": "string" }
        }
      }
    },
    "average_rating": { "title": "Genomsnittligt betyg", "type": "string" },
    "backordered": { "title": "Restnoterad", "type": "boolean" },
    "backorders": { "title": "Restnoteringar tillåtna (val)", "type": "string" },
    "backorders_allowed": { "title": "Restnoteringar tillåtna", "type": "boolean" },
    "button_text": { "title": "Knapptext", "type": "string" },
    "catalog_visibility": { "title": "Synlighet i katalog", "type": "string" },
    "categories": {
      "title": "Kategorier",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "title": "Kategori-ID", "type": "integer" },
          "name": { "title": "Namn", "type": "string" },
          "slug": { "title": "Slug", "type": "string" }
        }
      }
    },
    "cross_sell_ids": { "title": "ID:n för korsförsäljning", "type": "array", "items": { "type": "integer" } },
    "date_created": { "title": "Skapad datum", "format": "date-time", "type": "string" },
    "date_created_gmt": { "title": "Skapad datum (GMT)", "format": "date-time", "type": "string" },
    "date_modified": { "title": "Ändrad datum", "format": "date-time", "type": "string" },
    "date_modified_gmt": { "title": "Ändrad datum (GMT)", "format": "date-time", "type": "string" },
    "date_on_sale_from": { "title": "Reastart datum", "format": "date-time", "type": "string" },
    "date_on_sale_from_gmt": { "title": "Reastart datum (GMT)", "format": "date-time", "type": "string" },
    "date_on_sale_to": { "title": "Reaslut datum", "format": "date-time", "type": "string" },
    "date_on_sale_to_gmt": { "title": "Reaslut datum (GMT)", "format": "date-time", "type": "string" },
    "default_attributes": {
      "title": "Standardattribut",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "has_archives": { "title": "Har arkiv", "type": "boolean" },
          "id": { "title": "Attribut-ID", "type": "integer" },
          "name": { "title": "Namn", "type": "string" },
          "option": { "title": "Alternativ", "type": "string" },
          "order_by": { "title": "Sortering", "type": "string" },
          "slug": { "title": "Slug", "type": "string" },
          "type": { "title": "Typ", "type": "string" }
        }
      }
    },
    "description": { "title": "Beskrivning", "type": "string" },
    "dimensions": {
      "title": "Dimensioner",
      "type": "object",
      "properties": {
        "height": { "title": "Höjd", "type": "string" },
        "length": { "title": "Längd", "type": "string" },
        "width": { "title": "Bredd", "type": "string" }
      }
    },
    "download_expiry": { "title": "Giltighetstid för nedladdning", "type": "integer" },
    "download_limit": { "title": "Nedladdningsgräns", "type": "integer" },
    "downloadable": { "title": "Nedladdningsbar", "type": "boolean" },
    "downloads": {
      "title": "Nedladdningsbara filer",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "file": { "title": "Fil-URL", "type": "string" },
          "id": { "title": "Fil-ID", "type": "string" },
          "name": { "title": "Filnamn", "type": "string" }
        }
      }
    },
    "external_url": { "title": "Extern URL", "type": "string" },
    "featured": { "title": "Utvald produkt", "type": "boolean" },
    "grouped_products": { "title": "Grupperade produkt-ID:n", "type": "array", "items": { "type": "integer" } },
    "id": { "title": "Unikt ID", "type": "integer" },
    "images": {
      "title": "Bilder",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "alt": { "title": "Alternativ text", "type": "string" },
          "date_created": { "title": "Skapad datum", "type": "string", "format": "date-time" },
          "date_created_gmt": { "title": "Skapad datum (GMT)", "type": "string", "format": "date-time" },
          "date_modified": { "title": "Ändrad datum", "type": "string", "format": "date-time" },
          "date_modified_gmt": { "title": "Ändrad datum (GMT)", "type": "string", "format": "date-time" },
          "id": { "title": "Bild-ID", "type": "integer" },
          "name": { "title": "Namn", "type": "string" },
          "src": { "title": "Bildkälla (URL)", "type": "string" }
        }
      }
    },
    "manage_stock": { "title": "Hantera lager", "type": "boolean" },
    "menu_order": { "title": "Menyordning", "type": "integer" },
    "meta_data": {
      "title": "Metadata",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "display_key": { "title": "Visningsnyckel", "type": "string" },
          "display_value": { "title": "Visningsvärde", "type": "object" },
          "id": { "title": "Meta-ID", "type": "integer" },
          "key": { "title": "Nyckel", "type": "string" },
          "value": { "title": "Värde", "type": "object" }
        }
      }
    },
    "name": { "title": "Produktnamn", "type": "string" },
    "on_sale": { "title": "På rea", "type": "boolean" },
    "parent_id": { "title": "Förälder-ID", "type": "integer" },
    "permalink": { "title": "Permanent länk", "type": "string" },
    "price": { "title": "Pris", "type": "string" },
    "price_html": { "title": "Pris (HTML)", "type": "string" },
    "productId": { "title": "Produkt-ID", "type": "integer" },
    "purchasable": { "title": "Går att köpa", "type": "boolean" },
    "purchase_note": { "title": "Köpinformation", "type": "string" },
    "rating_count": { "title": "Antal betyg", "type": "integer" },
    "regular_price": { "title": "Ordinarie pris", "type": "string" },
    "related_ids": { "title": "Relaterade ID:n", "type": "array", "items": { "type": "integer" } },
    "reviews_allowed": { "title": "Recensioner tillåtna", "type": "boolean" },
    "sale_price": { "title": "Reapris", "type": "string" },
    "shipping_class": { "title": "Fraktklass (slug)", "type": "string" },
    "shipping_class_id": { "title": "Fraktklass-ID", "type": "integer" },
    "shipping_required": { "title": "Frakt krävs", "type": "boolean" },
    "shipping_taxable": { "title": "Frakten är skattepliktig", "type": "boolean" },
    "short_description": { "title": "Kort beskrivning", "type": "string" },
    "sku": { "title": "Artikelnummer (SKU)", "type": "string" },
    "slug": { "title": "Slug", "type": "string" },
    "sold_individually": { "title": "Säljs styckvis", "type": "boolean" },
    "status": { "title": "Status", "type": "string" },
    "stock_quantity": { "title": "Lagerantal", "type": "integer" },
    "stock_status": { "title": "Lagerstatus", "type": "string" },
    "tags": {
      "title": "Taggar",
      "type": "array",
      "items": {
        "type": "object",
        "required": ["name"],
        "properties": {
          "count": { "title": "Antal produkter", "type": "integer" },
          "description": { "title": "Beskrivning", "type": "string" },
          "id": { "title": "Tagg-ID", "type": "integer" },
          "name": { "title": "Namn", "type": "string" },
          "slug": { "title": "Slug", "type": "string" }
        }
      }
    },
    "tax_class": { "title": "Skatteklass", "type": "string" },
    "tax_status": { "title": "Skattestatus", "type": "string" },
    "total_sales": { "title": "Total försäljning", "format": "int64", "type": "integer" },
    "type": { "title": "Produkttyp", "type": "string" },
    "upsell_ids": { "title": "Mersäljs-ID:n", "type": "array", "items": { "type": "integer" } },
    "variations": { "title": "Varianter (ID:n)", "type": "array", "items": { "type": "integer" } },
    "virtual": { "title": "Virtuell produkt", "type": "boolean" },
    "weight": { "title": "Vikt", "type": "string" }
  }
}
