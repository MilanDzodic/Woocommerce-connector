{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "type": "object",
  "required": [
    "customerId"
  ],
  "properties": {
    "customerId": {
      "type": "integer",
      "title": "Kund-ID"
    },
    "id": {
      "type": "integer",
      "title": "ID"
    },
    "username": {
      "type": "string",
      "title": "Användarnamn"
    },
    "email": {
      "type": "string",
      "title": "E-postadress"
    },
    "first_name": {
      "type": "string",
      "title": "Förnamn"
    },
    "last_name": {
      "type": "string",
      "title": "Efternamn"
    },
    "role": {
      "type": "string",
      "title": "Roll"
    },
    "password": {
      "type": "string",
      "title": "Lösenord"
    },
    "date_created": {
      "type": "string",
      "format": "date-time",
      "title": "Skapad datum"
    },
    "date_created_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Skapad datum (GMT)"
    },
    "date_modified": {
      "type": "string",
      "format": "date-time",
      "title": "Ändrad datum"
    },
    "date_modified_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Ändrad datum (GMT)"
    },
    "is_paying_customer": {
      "type": "boolean",
      "title": "Är betalande kund"
    },
    "avatar_url": {
      "type": "string",
      "title": "Avatar-URL"
    },
    "billing": {
      "type": "object",
      "title": "Faktureringsuppgifter",
      "properties": {
        "first_name": { "type": "string", "title": "Förnamn" },
        "last_name": { "type": "string", "title": "Efternamn" },
        "company": { "type": "string", "title": "Företag" },
        "address_1": { "type": "string", "title": "Adressrad 1" },
        "address_2": { "type": "string", "title": "Adressrad 2" },
        "city": { "type": "string", "title": "Stad" },
        "state": { "type": "string", "title": "Delstat/Län" },
        "postcode": { "type": "string", "title": "Postnummer" },
        "country": { "type": "string", "title": "Land (ISO-kod)" },
        "email": { "type": "string", "title": "E-post" },
        "phone": { "type": "string", "title": "Telefon" }
      }
    },
    "shipping": {
      "type": "object",
      "title": "Leveransuppgifter",
      "properties": {
        "first_name": { "type": "string", "title": "Förnamn" },
        "last_name": { "type": "string", "title": "Efternamn" },
        "company": { "type": "string", "title": "Företag" },
        "address_1": { "type": "string", "title": "Adressrad 1" },
        "address_2": { "type": "string", "title": "Adressrad 2" },
        "city": { "type": "string", "title": "Stad" },
        "state": { "type": "string", "title": "Delstat/Län" },
        "postcode": { "type": "string", "title": "Postnummer" },
        "country": { "type": "string", "title": "Land (ISO-kod)" },
        "phone": { "type": "string", "title": "Telefon" }
      }
    },
    "meta_data": {
      "type": "array",
      "title": "Metadata",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Meta-ID" },
          "key": { "type": "string", "title": "Nyckel" },
          "value": { "type": "object", "title": "Värde", "properties": {} },
          "display_key": { "type": "string", "title": "Visningsnyckel" },
          "display_value": { "type": "object", "title": "Visningsvärde", "properties": {} }
        }
      }
    }
  }
}
