{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "customerId"
  ],
  "properties": {
    "customerId": {
      "type": "integer",
      "title": "Kund-ID",
      "description": "Det unika ID:t för kunden i WooCommerce."
    },
    "on_not_found": {
    "type": "string",
    "title": "Hantering vid saknat resultat",
    "default": "fail",
    "oneOf": [
      { "const": "fail", "title": "Misslyckas (stoppa körningen)" },
      { "const": "continue", "title": "Fortsätt med tomt resultat" },
      { "const": "exit_level", "title": "Avbryt nuvarande nivå" },
      { "const": "exit_execution", "title": "Avbryt hela körningen" }
      ]
    }
  }
}
