{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "title": "ID"
    },
    "name": {
      "type": "string",
      "title": "Produktnamn"
    },
    "slug": {
      "type": "string",
      "title": "Slugg"
    },
    "permalink": {
      "type": "string",
      "title": "Permanent länk"
    },
    "date_created": {
      "type": "string",
      "format": "date-time",
      "title": "Skapad datum"
    },
    "date_created_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Skapad datum (GMT)"
    },
    "date_modified": {
      "type": "string",
      "format": "date-time",
      "title": "Ändrad datum"
    },
    "date_modified_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Ändrad datum (GMT)"
    },
    "type": {
      "type": "string",
      "title": "Produkttyp"
    },
    "status": {
      "type": "string",
      "title": "Status"
    },
    "featured": {
      "type": "boolean",
      "title": "Framhävd produkt"
    },
    "catalog_visibility": {
      "type": "string",
      "title": "Synlighet i katalog"
    },
    "description": {
      "type": "string",
      "title": "Beskrivning"
    },
    "short_description": {
      "type": "string",
      "title": "Kort beskrivning"
    },
    "sku": {
      "type": "string",
      "title": "SKU (Artikelnr)"
    },
    "price": {
      "type": "string",
      "title": "Pris"
    },
    "regular_price": {
      "type": "string",
      "title": "Ordinarie pris"
    },
    "sale_price": {
      "type": "string",
      "title": "Reapris"
    },
    "date_on_sale_from": {
      "type": "string",
      "format": "date-time",
      "title": "Rea startdatum"
    },
    "date_on_sale_from_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Rea startdatum (GMT)"
    },
    "date_on_sale_to": {
      "type": "string",
      "format": "date-time",
      "title": "Rea slutdatum"
    },
    "date_on_sale_to_gmt": {
      "type": "string",
      "format": "date-time",
      "title": "Rea slutdatum (GMT)"
    },
    "price_html": {
      "type": "string",
      "title": "Pris (HTML)"
    },
    "on_sale": {
      "type": "boolean",
      "title": "På rea"
    },
    "purchasable": {
      "type": "boolean",
      "title": "Köpbart"
    },
    "total_sales": {
      "type": "integer",
      "format": "int64",
      "title": "Totalt sålda"
    },
    "virtual": {
      "type": "boolean",
      "title": "Virtuell"
    },
    "downloadable": {
      "type": "boolean",
      "title": "Nedladdningsbar"
    },
    "downloads": {
      "type": "array",
      "title": "Nedladdningar",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "title": "Fil-ID" },
          "name": { "type": "string", "title": "Filnamn" },
          "file": { "type": "string", "title": "Fil-URL" }
        }
      }
    },
    "download_limit": {
      "type": "integer",
      "title": "Nedladdningsgräns"
    },
    "download_expiry": {
      "type": "integer",
      "title": "Giltighetstid för nedladdning"
    },
    "external_url": {
      "type": "string",
      "title": "Extern URL"
    },
    "button_text": {
      "type": "string",
      "title": "Knapptext"
    },
    "tax_status": {
      "type": "string",
      "title": "Skattestatus"
    },
    "tax_class": {
      "type": "string",
      "title": "Skatteklass"
    },
    "manage_stock": {
      "type": "boolean",
      "title": "Hantera lager"
    },
    "stock_quantity": {
      "type": "integer",
      "title": "Lagersaldo"
    },
    "stock_status": {
      "type": "string",
      "title": "Lagerstatus"
    },
    "backorders": {
      "type": "string",
      "title": "Restnoteringar"
    },
    "backorders_allowed": {
      "type": "boolean",
      "title": "Restnoteringar tillåtna"
    },
    "backordered": {
      "type": "boolean",
      "title": "Är restnoterad"
    },
    "sold_individually": {
      "type": "boolean",
      "title": "Säljs styckvis"
    },
    "weight": {
      "type": "string",
      "title": "Vikt"
    },
    "dimensions": {
      "type": "object",
      "title": "Dimensioner",
      "properties": {
        "length": { "type": "string", "title": "Längd" },
        "width": { "type": "string", "title": "Bredd" },
        "height": { "type": "string", "title": "Höjd" }
      }
    },
    "shipping_required": {
      "type": "boolean",
      "title": "Frakt krävs"
    },
    "shipping_taxable": {
      "type": "boolean",
      "title": "Frakt är skattepliktig"
    },
    "shipping_class": {
      "type": "string",
      "title": "Fraktklass"
    },
    "shipping_class_id": {
      "type": "integer",
      "title": "Fraktklass-ID"
    },
    "reviews_allowed": {
      "type": "boolean",
      "title": "Recensioner tillåtna"
    },
    "average_rating": {
      "type": "string",
      "title": "Genomsnittligt betyg"
    },
    "rating_count": {
      "type": "integer",
      "title": "Antal betyg"
    },
    "related_ids": {
      "type": "array",
      "title": "Relaterade produkt-ID:n",
      "items": { "type": "integer" }
    },
    "upsell_ids": {
      "type": "array",
      "title": "Mersäljs-ID:n (Upsells)",
      "items": { "type": "integer" }
    },
    "cross_sell_ids": {
      "type": "array",
      "title": "Korssäljs-ID:n (Cross-sells)",
      "items": { "type": "integer" }
    },
    "parent_id": {
      "type": "integer",
      "title": "Moderprodukt-ID"
    },
    "purchase_note": {
      "type": "string",
      "title": "Köpnotering"
    },
    "categories": {
      "type": "array",
      "title": "Kategorier",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Kategori-ID" },
          "name": { "type": "string", "title": "Kategorinamn" },
          "slug": { "type": "string", "title": "Kategori-slugg" }
        }
      }
    },
    "tags": {
      "type": "array",
      "title": "Taggar",
      "items": {
        "type": "object",
        "required": ["name"],
        "properties": {
          "id": { "type": "integer", "title": "Tagg-ID" },
          "name": { "type": "string", "title": "Taggnamn" },
          "slug": { "type": "string", "title": "Tagg-slugg" },
          "description": { "type": "string", "title": "Taggbeskrivning" },
          "count": { "type": "integer", "title": "Antal produkter" }
        }
      }
    },
    "images": {
      "type": "array",
      "title": "Bilder",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Bild-ID" },
          "date_created": { "type": "string", "format": "date-time", "title": "Skapad" },
          "date_created_gmt": { "type": "string", "format": "date-time", "title": "Skapad (GMT)" },
          "date_modified": { "type": "string", "format": "date-time", "title": "Ändrad" },
          "date_modified_gmt": { "type": "string", "format": "date-time", "title": "Ändrad (GMT)" },
          "src": { "type": "string", "title": "Bildkälla (URL)" },
          "name": { "type": "string", "title": "Bildnamn" },
          "alt": { "type": "string", "title": "Alt-text" }
        }
      }
    },
    "attributes": {
      "type": "array",
      "title": "Attribut",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Attribut-ID" },
          "name": { "type": "string", "title": "Attributnamn" },
          "slug": { "type": "string", "title": "Attribut-slugg" },
          "option": { "type": "string", "title": "Valt alternativ" },
          "has_archives": { "type": "boolean", "title": "Har arkiv" },
          "order_by": { "type": "string", "title": "Sortering" },
          "type": { "type": "string", "title": "Typ" }
        }
      }
    },
    "default_attributes": {
      "type": "array",
      "title": "Standardattribut",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Attribut-ID" },
          "name": { "type": "string", "title": "Attributnamn" },
          "slug": { "type": "string", "title": "Attribut-slugg" },
          "option": { "type": "string", "title": "Valt alternativ" },
          "has_archives": { "type": "boolean", "title": "Har arkiv" },
          "order_by": { "type": "string", "title": "Sortering" },
          "type": { "type": "string", "title": "Typ" }
        }
      }
    },
    "variations": {
      "type": "array",
      "title": "Varianter",
      "items": { "type": "integer" }
    },
    "grouped_products": {
      "type": "array",
      "title": "Grupperade produkter",
      "items": { "type": "integer" }
    },
    "menu_order": {
      "type": "integer",
      "title": "Menyordning"
    },
    "meta_data": {
      "type": "array",
      "title": "Metadata",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer", "title": "Meta-ID" },
          "key": { "type": "string", "title": "Nyckel" },
          "value": { "type": "object", "title": "Värde", "properties": {} },
          "display_key": { "type": "string", "title": "Visningsnyckel" },
          "display_value": { "type": "object", "title": "Visningsvärde", "properties": {} }
        }
      }
    }
  }
}
